$('#dialog-modal').html("<%= escape_javascript( render(:partial => "new" ) ) %>");
$('#dialog-modal').dialog({
	title: 'Add Game Result',
	height: 400,
	width: 500,
	modal: true,
	buttons: {
		Seave: function() {
			$(this).children("form").submit();
		},
		Cancel: function() {
			$(this).dialog("close");
		}
	}
});