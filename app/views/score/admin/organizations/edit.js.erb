$('#dialog-modal').html("<%= escape_javascript( render(:partial => "form" ) ) %>");
$('#dialog-modal').dialog({
	title: 'Edit Organization',
	height: 300,
	width: 400,
	modal: true,
	buttons: {
		Save: function() {
			$(this).children("form").submit();
		},
		Cancel: function() {
			$(this).dialog("close");
		}
	}
});