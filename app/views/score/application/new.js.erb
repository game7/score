$('#dialog-modal').html("<%= escape_javascript( render(:partial => "form" ) ) %>");
$('#dialog-modal').dialog({
	title: 'New <%= @inflections[:singular] %>',
	height: 'auto',
	width: 400,
	modal: true,
	buttons: {
		Save: function() {
			$(this).children("form").submit();
		},
		Cancel: function() {
			$(this).dialog("close");
		}
	}
});